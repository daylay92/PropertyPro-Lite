{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base","prettier"
    ],
    "plugins": [
        "prettier","mocha"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "describe": "readonly",
        "it": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console" : 0,
        "mocha/no-exclusive-tests": "error"
    }
}